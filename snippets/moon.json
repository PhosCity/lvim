{
  "func": {
    "prefix": "func",
    "body": [
      "$1 = ($2) ->\n  $3"
    ],
    "description": "function"
  },
  "aegi": {
    "prefix": "export",
    "body": [
      "export script_name = \"${1:copy}\"\nexport script_description = \"$2\"\nexport script_version = \"0.0.1\"\nexport script_author = \"PhosCity\"\nexport script_namespace = \"$4phos.$1\"\n\n$5"
    ],
    "description": "template for aegi scripts"
  },
  "depctrl": {
    "prefix": "depctrl",
    "body": [
"
DependencyControl = require \"l0.DependencyControl\"\ndepctrl = DependencyControl{\n  feed: \"\",\n  {\n  }\n}\nLineCollection, ASS = depctrl\\\\requireModules!\nlogger = depctrl\\\\getLogger!\n\n$1
"
    ],
    "description": "template for dependecny control"
  },
  "register": {
    "prefix": "register",
    "body": [
      "depctrl\\\\registerMacro $1"
    ],
    "description": "register macros"
  },
  "registermacros": {
    "prefix": "registermacros",
    "body": [
      "depctrl\\\\registerMacros({\n	{ \"$1\", \"$2\", $3 },\n})"
    ],
    "description": "register macros"
  },
  "list": {
    "prefix": "list",
    "body": [
      "$1 = { $2 }"
    ],
    "description": "list"
  },
  "table": {
    "prefix": "table",
    "body": [
      "$1 = {\n $2: $3\n $4\n}"
    ],
    "description": "table"
  },
  "ipairs": {
    "prefix": "ipairs",
    "body": [
      "for index, item in ipairs $1\n  $2"
    ],
    "description": "ipairs like lua"
  },
  "ipairs2": {
    "prefix": "ipairs2",
    "body": [
      "for item in *$1\n  $2"
    ],
    "description": "iparis when index is not needed"
  },
  "pairs": {
    "prefix": "pairs",
    "body": [
      "for key, value in pairs $1\n  $2"
    ],
    "description": "pairs like lua"
  },
  "while": {
    "prefix": "while",
    "body": [
      "while $1\n  $2"
    ],
    "description": "while loop"
  },
  "continue": {
    "prefix": "conif",
    "body": [
      "continue if $1"
    ],
    "description": "continue if condition"
  },
  "continue2": {
    "prefix": "conun",
    "body": [
      "continue unless $1"
    ],
    "description": "continue unless condition"
  },
  "if": {
    "prefix": "if",
    "body": [
      "if $1\n  $2"
    ],
    "description": "if condition"
  },
  "ifelse": {
    "prefix": "ifelse",
    "body": [
      "if $1\n  $2\nelse\n  $3"
    ],
    "description": "if condition else"
  },
  "ifelseif": {
    "prefix": "ifelseif",
    "body": [
      "if $1\n  $2\nelseif\n  $3"
    ],
    "description": "if condition elseif condition"
  },
  "unless": {
    "prefix": "unless",
    "body": [
      "unless $1\n  $2"
    ],
    "description": "unless condition"
  },
  "switch": {
    "prefix": "switch",
    "body": [
      "switch $1\n  when $2"
    ],
    "description": "switch"
  }
}
